#include <stdio.h>
#include <string.h>
#include <time.h>

int Proc_response(char *in_str, char *out_str){
	int t=time(NULL)%10;
	if ( strcmp("init", in_str) ==  0){
		strcpy(out_str, "こんにちは！ボクおっくんだよ！\n");
		return 0;
	}
	switch (t){
		case 0:
			strcpy(out_str, "牛が食いたくば倒すのみ！\n");
			break;
	}
	return 0;
}

